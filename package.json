    "@types/ramda": "^0.27.0",
    "@webcarrot/xirr": "^1.0.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.1",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.3",
    "apollo-utilities": "^1.3.3",
    "axios": "^0.19.2",
    "bignumber.js": "^9.0.0",
    "cross-env": "^7.0.2",
    "dotenv-extended": "^2.7.1",
    "graphql": "^14.6.0",
