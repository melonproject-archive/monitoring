    "next-with-apollo": "^5.0.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-no-ssr": "^1.1.0",
    "react-query": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@next/bundle-analyzer": "^9.3.1",
    "@types/jest": "^25.1.4",
    "@types/next": "^9.0.0",
    "@types/recharts": "^1.8.9",
    "babel-jest": "^25.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-environment-jsdom": "^25.1.0",
    "lint-staged": "^10.0.9",
    "next": "^9.3.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn typecheck && yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format"
    ]
  }
